@{
    ViewData["Title"] = "Home Page";
}
<div>
    <h1>Bem vindo a sua nova forma de se locomover</h1>
    <h2>Vai de BAZE!</h2>
</div>


<div id="carouselExample" class="carousel slide">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://media.istockphoto.com/id/1402134774/photo/professional-road-cyclist-on-a-training-ride.jpg?s=1024x1024&w=is&k=20&c=_09YNPNzheMtTEJ0E9I68N06q_-E6vZwvQyHlz9qi8k=" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://media.istockphoto.com/id/1326106135/photo/close-up-of-man-riding-bike.jpg?s=1024x1024&w=is&k=20&c=PG9qP1u5lnyQVHYZQE27wbXrotgczrXS1Gple-ch0rg=" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://media.istockphoto.com/id/1340571998/photo/man-rides-a-bike-outdoors-in-the-park-on-a-sunny-day-at-sunset.jpg?s=1024x1024&w=is&k=20&c=BA3Ky-0B_S0SFPBCJd4FPRxZNIDtTXO0Xz7c7Cvyw7A=" class="d-block w-100" alt="...">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>